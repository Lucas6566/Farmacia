program Farmacia;

uses
  Vcl.Forms,
  Farmacia.View.Principal in 'View\Farmacia.View.Principal.pas' {FormPrincipal},
  Farmacia.Service.Conexao in 'Service\Farmacia.Service.Conexao.pas' {FormConexao},
  Farmacia.View.Pesquisa.Pai in 'View\Pesquisa\Farmacia.View.Pesquisa.Pai.pas' {FormPesquisaPai},
  Farmacia.View.Pesquisa.Cliente in 'View\Pesquisa\Farmacia.View.Pesquisa.Cliente.pas' {FormPesquisaCliente},
  Farmacia.View.Cadastro.Pai in 'View\Cadastro\Farmacia.View.Cadastro.Pai.pas' {FormCadastroPai},
  Farmacia.View.Cadastro.Cliente in 'View\Cadastro\Farmacia.View.Cadastro.Cliente.pas' {FormCadastroCliente},
  Farmacia.View.Pesquisa.Funcionario in 'View\Pesquisa\Farmacia.View.Pesquisa.Funcionario.pas' {FormPesquisaFuncionario},
  Farmacia.View.Cadastro.Funcionario in 'View\Cadastro\Farmacia.View.Cadastro.Funcionario.pas' {FormCadastroFuncionario},
  Farmacia.Util.Types in 'Util\Farmacia.Util.Types.pas',
  Farmacia.Util.FunctionSQL in 'Util\Farmacia.Util.FunctionSQL.pas';

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TFormPrincipal, FormPrincipal);
  Application.CreateForm(TFormConexao, FormConexao);
  Application.CreateForm(TFormCadastroPai, FormCadastroPai);
  Application.CreateForm(TFormCadastroCliente, FormCadastroCliente);
  Application.CreateForm(TFormPesquisaFuncionario, FormPesquisaFuncionario);
  Application.CreateForm(TFormCadastroFuncionario, FormCadastroFuncionario);
  Application.Run;
end.
