program Farmacia;

uses
  Vcl.Forms,
  Farmacia.View.Principal in 'View\Farmacia.View.Principal.pas' {Form1},
  Farmacia.Service.Conexao in 'Service\Farmacia.Service.Conexao.pas' {Form3},
